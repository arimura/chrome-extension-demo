{
    "manifest_version": 3,
    "name": "Input Suggestion Extension",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "permissions": [
      "activeTab"
    ],
    "devtools_page": "devtools.html",
    "content_scripts": [
      {
        "matches": ["https://*/*", "http://*/*"],
        "js": ["content.js"]
      }
    ]
  }
  